#include<bits/stdc++.h>
using namespace std;
typedef long long ll;
int main()
{

    ll s,f;
    cin >> s >> f;
    ll a,b;

cin >> a >> b;
ll m,n;
cin >> m >> n;
vector <ll>v1(m);
vector <ll>v2(n);
for(ll i=0;i<m;i++)
    cin >> v1[i];
for(ll i=0;i<n;i++)
    cin >> v2[i];
ll count1=0,count2=0;
for(ll i=0;i<m;i++)
{
    ll d=s-a;
    if(v1[i]>=d && v1[i]<=d+f-s)
        count1++;

}
for(ll i=0;i<n;i++)
{
    ll d=f-b;
    if(v2[i]<0 && v2[i]<=d && v2[i]>=d+s-f)
        count2++;

}
cout << count1 << "\n";
cout << count2 << "\n";


    }



