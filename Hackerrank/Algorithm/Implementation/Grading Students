#include<bits/stdc++.h>
using namespace std;
int main()
{

    int n;
    cin >> n;
    vector <int > v(n);
    for(int i=0;i<n;i++)
    {
        cin >> v[i];
    }

    for(int i=0;i<n;i++)
    {
        if(v[i]<=37)
            cout << v[i] << "\n";
        else
        {
            int rem=v[i]%5;
            if(rem>2)
            {
                int roun=5-(v[i]%5);
                cout <<  v[i]+roun<< "\n";}
            else
                 cout << v[i] << "\n";




        }


    }


}
