#include <bits/stdc++.h>

using namespace std;
#define all(c) (c).begin(),(c).end()
int main()
{
      int n;
    cin >> n;
    vector <int > v(n);
  
    for(int i=0;i<n;i++)
        cin >> v[i];
    sort(all(v));
    int ma=*max_element(all(v));
   int low=lower_bound(all(v),ma)-v.begin();
    int upp=upper_bound(all(v),ma)-v.begin();
    cout << upp-low << "\n";
    
    
    
    
}


