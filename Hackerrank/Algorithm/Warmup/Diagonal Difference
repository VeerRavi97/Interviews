#include<bits/stdc++.h>
using namespace std;
typedef long long ll;
typedef vector < ll > vi;
int main()
{
    int n;

    vector < vi > vvi;
    vi v1;
    vi v2;
cin >>n;
vvi.assign(n, vi());
      for(int i=0;i<n;i++)
        {
      for(int j=0;j<n;j++)
        {
            ll in;
            cin >> in;
            vvi[i].push_back(in);

        }
    }
    for(int i=0;i<n;i++)
    {
        v1.push_back(vvi[i][i]);
        v2.push_back(vvi[i][n-i-1]);
    }

    ll sum1=0,sum2=0;

    for(int i=0;i<n;i++)
        sum1+=v1[i];
    for(int i=0;i<n;i++)
        sum2+=v2[i];
    ll diff=abs(sum1-sum2);
    cout << diff << "\n";







}
