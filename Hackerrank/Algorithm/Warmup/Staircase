#include<bits/stdc++.h>
using namespace std;


int main(){
    int n;
    cin >> n;
    
    for(int i=1;i<=n;i++)
    {
        int diff=n-i;
        for(int j=0;j<diff;j++)
        cout << " ";
        for(int k=1;k<=i;k++)
            cout << "#" ;
        cout << "\n";
        
    }
    return 0;
}
